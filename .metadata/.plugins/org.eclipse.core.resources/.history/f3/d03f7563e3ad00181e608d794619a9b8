import java.util.Scanner;

public class trianglestouch {

	public static void main(String[] args) {
		// triangles not touch-true
		// rest all false
		Scanner scn = new Scanner(System.in);
		
		int x1 = scn.nextInt();
		int x2 = scn.nextInt();
		int x3 = scn.nextInt();
		int x4 = scn.nextInt();
		int y1 = scn.nextInt();
		int y2 = scn.nextInt();
		int y3 = scn.nextInt();
		int y4 = scn.nextInt();
		boolean bn=true;
		
		if((x1>=x3&&x3<=x2)&&(x1>=x4&&x3<=x4)&&(y1>=y3&&y3<=y2)&&(y1>=y4&&y3<=y4)) {
		bn=true;
		}	
		else if((x1>=x3&&x3<=x2)||(x1>=x4&&x3<=x4)||(y1>=y3&&y3<=y2)||(y1>=y4&&y3<=y4)) {
			bn =false;
		}else {bn=true;}
		if(bn=false) {System.out.println("Triangles Touch");}
	}
}
