
public class recursion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		pdiwskip(5);
	}//expectation=>
	//faith=>
	public static void pdiwskip(int n ) {
		if(n==0) {
			return;
		}
		if(n%2 != 0) {
		System.out.println(n);}
		pdiwskip(n-1);
		if(n%2==0) {
		System.out.println(n);}
	}

}
