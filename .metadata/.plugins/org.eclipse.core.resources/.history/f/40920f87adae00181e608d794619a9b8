import java.util.Scanner;

public class reverse {

	public static void main(String[] args) {
		int[] arr = { 100, 50, 100, 20, 99, 33, 66 };
		
		//logic
		int hi=arr.length-1;
		int lo =0;
		
		while(lo<=hi) {
			int temp = arr[lo];
			arr[lo]=arr[hi];
			arr[hi]=temp;
			lo++;
			hi--;
		}
		
		
		for(int val: arr) {
			System.out.print(val+" ");
		}
	}

}
