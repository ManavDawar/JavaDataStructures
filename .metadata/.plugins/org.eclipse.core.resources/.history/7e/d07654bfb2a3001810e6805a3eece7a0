package test;

import java.util.Scanner;

public class test1 {

	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		
		System.out.println("Enter a Number");
		int n = scn.nextInt();
		int cntr=1;
		while(cntr<=n) {
			if(n/cntr=2) {
				System.out.println("It is not prime");
			}else {
				System.out.println("It is Prime.");
			}
		}
		

	}

}
