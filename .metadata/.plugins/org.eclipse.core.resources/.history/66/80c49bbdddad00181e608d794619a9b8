import java.util.Scanner;

public class booleanupdown {

	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		
		int n = scn.nextInt();
		int prev =0;
		boolean b = true;
		
		for(int i=0;i<n;i++) {
			int curr =scn.nextInt();
			if(curr<prev) {
				b = true;
			}else { b = false;
			
			}
			System.out.println(b);
			
			
		}
		

	}

}
