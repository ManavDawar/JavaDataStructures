package practice;

public class prac1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = { 10, 20, 30 };
		subset(arr);
	}

	public static void subset(int[] arr) {
		for (int i = 0; i < (1<<arr.length); i++) {
			for(int j=arr.length-1;j>=0;j++){
				if((i & (j))!= 0) {
					System.out.print(arr[j]+" ");
				}
			}System.out.println();
		}

	}

}
