import java.util.Scanner;

public class pattern4 {

	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		
		int n = scn.nextInt();
		
		int cntr=1;
		int c =1;
		int z = 0;
		while(cntr<=n) {
			int col = 1;
			while(col<=cntr) {
				System.out.print(z+" ");
				
				z=c*(cntr-col)/(col+1);
				col++;
				
			}
			System.out.println();
			cntr++;
		}

	}

}
