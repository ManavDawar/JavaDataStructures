
public class subarraysum {

	public static void main(String[] args) {

		int[] arr = { 4, 5, -12, 3, 6, -2, 5, -20, 1, 2, 8 };
		subarraysum1(arr);

	}

	public static void subarraysum1(int[] arr) {
		int max=0;
		for(int i=0;i<arr.length;i++) {
			max=max+arr[i];
			if(max>max+arr[i+1]) {
				max=max;
			}else{max=0;}
		}System.out.println(max);

	}

}
