
public class arrayshift {

	public static void main(String[] args) {

		int[] arr = { 33, 66, 55, 77, 88 };

		int n = 2;

		int hi=arr.length-n-1;
		int lo =0;
		
		while(lo<hi) {
			int temp = arr[lo];
			arr[lo]=arr[hi];
			arr[hi]=temp;
			lo++;
			hi--;
		}

		 hi=arr.length-1;
		 lo =arr.length-n;
		
		while(lo<hi) {
			int temp = arr[lo];
			arr[lo]=arr[hi];
			arr[hi]=temp;
			lo++;
			hi--;
		}
		
		hi=arr.length-1;
		lo =0;
		
		while(lo<hi) {
			int temp = arr[lo];
			arr[lo]=arr[hi];
			arr[hi]=temp;
			lo++;
			hi--;
		}
		
		for(int val: arr) {
			System.out.print(val+" ");
		}
		
	}

}
