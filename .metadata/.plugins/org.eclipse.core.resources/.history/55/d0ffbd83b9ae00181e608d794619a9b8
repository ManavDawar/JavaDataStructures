
public class subarrays {

	public static void main(String[] args) {

		int[] arr = { 10, 20, 30, 40 };
		subarray(arr);

	}

	public static void subarray(int[] arr) {
		for (int i = 0; i < arr.length; i++) {
			for (int j = i+1; j < arr.length; j++) {
				for(int k = j;k<arr.length;k++) {
					System.out.print(arr[k]);
				}
				System.out.println();
			}
		}
	}

}
